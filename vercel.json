{
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "functions": {
        "app\\.js": {
            "memory": 3008,
            "maxDuration": 300
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]


 
}  